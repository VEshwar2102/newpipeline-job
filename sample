pipeline {
    agent any
    stages {
        stage('Test') {
            when { branch "master" }
             steps {
                    git branch: 'master', url: 'https://github.com/VEshwar2102/newpipeline-job.git'
                    sh '''
                        echo "This is a test1 stage"
                    ''' 
            }
             steps{
                     git branch: 'master', url: 'https://github.com/VEshwar2102/newpipeline-job.git'
                    sh '''
                        echo "This is a test2 stage"
                    ''' 
            }
        }
    }
}
